[
    
    {
        "id": 105,
        "name": "الفيل",
        "transliteration": "Al-Fil",
        "translation": "ہاتھی",
        "type": "meccan",
        "total_verses": 5,
        "verses": [
            {
                "id": 1,
                "text": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِأَصۡحَٰبِ ٱلۡفِيلِ",
                "translation": "تم نے دیکھا نہیں کہ تمہارے رب نے ہاتھی والوں کے ساتھ کیا کیا؟"
            },
            {
                "id": 2,
                "text": "أَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِي تَضۡلِيلٖ",
                "translation": "کیا اُس نے اُن کی تدبیر کو اکارت نہیں کر دیا؟"
            },
            {
                "id": 3,
                "text": "وَأَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا أَبَابِيلَ",
                "translation": "اور اُن پر پرندوں کے جھنڈ کے جھنڈ بھیج دیے"
            },
            {
                "id": 4,
                "text": "تَرۡمِيهِم بِحِجَارَةٖ مِّن سِجِّيلٖ",
                "translation": "جو اُن پر پکی ہوئی مٹی کے پتھر پھینک رہے تھے"
            },
            {
                "id": 5,
                "text": "فَجَعَلَهُمۡ كَعَصۡفٖ مَّأۡكُولِۭ",
                "translation": "پھر اُن کا یہ حال کر دیا جیسے جانوروں کا کھایا ہوا بھوسا"
            }
        ]
    }
]
