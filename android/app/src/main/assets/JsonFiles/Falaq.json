[
    
    {
        "id": 113,
        "name": "الفلق",
        "transliteration": "Al-Falaq",
        "translation": "صبح",
        "type": "meccan",
        "total_verses": 5,
        "verses": [
            {
                "id": 1,
                "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلۡفَلَقِ",
                "translation": "کہو، میں پناہ مانگتا ہوں صبح کے رب کی"
            },
            {
                "id": 2,
                "text": "مِن شَرِّ مَا خَلَقَ",
                "translation": "ہر اُس چیز کے شر سے جو اُس نے پیدا کی ہے"
            },
            {
                "id": 3,
                "text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
                "translation": "اور رات کی تاریکی کے شر سے جب کہ وہ چھا جائے"
            },
            {
                "id": 4,
                "text": "وَمِن شَرِّ ٱلنَّفَّـٰثَٰتِ فِي ٱلۡعُقَدِ",
                "translation": "اور گرہوں میں پھونکنے والوں (یا والیوں) کے شر سے"
            },
            {
                "id": 5,
                "text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
                "translation": "اور حاسد کے شر سے جب کہ وہ حسد کرے"
            }
        ]
    }
]