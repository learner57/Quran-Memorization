[
    
    {
        "id": 111,
        "name": "المسد",
        "transliteration": "Al-Masad",
        "translation": "خوب بٹی ہوئی",
        "type": "meccan",
        "total_verses": 5,
        "verses": [
            {
                "id": 1,
                "text": "تَبَّتۡ يَدَآ أَبِي لَهَبٖ وَتَبَّ",
                "translation": "ٹوٹ گئے ابولہب کے ہاتھ اور نامراد ہو گیا وہ"
            },
            {
                "id": 2,
                "text": "مَآ أَغۡنَىٰ عَنۡهُ مَالُهُۥ وَمَا كَسَبَ",
                "translation": "اُس کا مال اور جو کچھ اس نے کمایا وہ اُس کے کسی کام نہ آیا"
            },
            {
                "id": 3,
                "text": "سَيَصۡلَىٰ نَارٗا ذَاتَ لَهَبٖ",
                "translation": "ضرور وہ شعلہ زن آگ میں ڈالا جائے گا"
            },
            {
                "id": 4,
                "text": "وَٱمۡرَأَتُهُۥ حَمَّالَةَ ٱلۡحَطَبِ",
                "translation": "اور (اُس کے ساتھ) اُس کی جورو بھی، لگائی بجھائی کرنے والی"
            },
            {
                "id": 5,
                "text": "فِي جِيدِهَا حَبۡلٞ مِّن مَّسَدِۭ",
                "translation": "اُس کی گردن میں مونجھ کی رسی ہوگی"
            }
        ]
    }
]