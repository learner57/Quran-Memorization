[
    
    {
        "id": 87,
        "name": "الأعلى",
        "transliteration": "Al-A'la",
        "translation": "اعلی",
        "type": "meccan",
        "total_verses": 19,
        "verses": [
            {
                "id": 1,
                "text": "سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
                "translation": "(اے نبیؐ) اپنے رب برتر کے نام کی تسبیح کرو"
            },
            {
                "id": 2,
                "text": "ٱلَّذِي خَلَقَ فَسَوَّىٰ",
                "translation": "جس نے پیدا کیا اور تناسب قائم کیا"
            },
            {
                "id": 3,
                "text": "وَٱلَّذِي قَدَّرَ فَهَدَىٰ",
                "translation": "جس نے تقدیر بنائی پھر راہ دکھائی"
            },
            {
                "id": 4,
                "text": "وَٱلَّذِيٓ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
                "translation": "جس نے نباتات اگائیں"
            },
            {
                "id": 5,
                "text": "فَجَعَلَهُۥ غُثَآءً أَحۡوَىٰ",
                "translation": "پھر اُن کو سیاہ کوڑا کرکٹ بنا دیا"
            },
            {
                "id": 6,
                "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰٓ",
                "translation": "ہم تمہیں پڑھوا دیں گے، پھر تم نہیں بھولو گے"
            },
            {
                "id": 7,
                "text": "إِلَّا مَا شَآءَ ٱللَّهُۚ إِنَّهُۥ يَعۡلَمُ ٱلۡجَهۡرَ وَمَا يَخۡفَىٰ",
                "translation": "سوائے اُس کے جو اللہ چاہے، وہ ظاہر کو بھی جانتا ہے اور جو کچھ پوشیدہ ہے اُس کو بھی"
            },
            {
                "id": 8,
                "text": "وَنُيَسِّرُكَ لِلۡيُسۡرَىٰ",
                "translation": "اور ہم تمہیں آسان طریقے کی سہولت دیتے ہیں"
            },
            {
                "id": 9,
                "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
                "translation": "لہٰذا تم نصیحت کرو اگر نصیحت نافع ہو"
            },
            {
                "id": 10,
                "text": "سَيَذَّكَّرُ مَن يَخۡشَىٰ",
                "translation": "جو شخص ڈرتا ہے وہ نصیحت قبول کر لے گا"
            },
            {
                "id": 11,
                "text": "وَيَتَجَنَّبُهَا ٱلۡأَشۡقَى",
                "translation": "اور اس سے گریز کریگا وہ انتہائی بد بخت"
            },
            {
                "id": 12,
                "text": "ٱلَّذِي يَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
                "translation": "جو بڑی آگ میں جائے گا"
            },
            {
                "id": 13,
                "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحۡيَىٰ",
                "translation": "پھر نہ اس میں مرے گا نہ جیے گا"
            },
            {
                "id": 14,
                "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
                "translation": "فلاح پا گیا وہ جس نے پاکیزگی اختیار کی"
            },
            {
                "id": 15,
                "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
                "translation": "اور اپنے رب کا نام یاد کیا پھر نماز پڑھی"
            },
            {
                "id": 16,
                "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَيَوٰةَ ٱلدُّنۡيَا",
                "translation": "مگر تم لوگ دنیا کی زندگی کو ترجیح دیتے ہو"
            },
            {
                "id": 17,
                "text": "وَٱلۡأٓخِرَةُ خَيۡرٞ وَأَبۡقَىٰٓ",
                "translation": "حالانکہ آخرت بہتر ہے اور باقی رہنے والی ہے"
            },
            {
                "id": 18,
                "text": "إِنَّ هَٰذَا لَفِي ٱلصُّحُفِ ٱلۡأُولَىٰ",
                "translation": "یہی بات پہلے آئے ہوئے صحیفوں میں بھی کہی گئی تھی"
            },
            {
                "id": 19,
                "text": "صُحُفِ إِبۡرَٰهِيمَ وَمُوسَىٰ",
                "translation": "ابراہیمؑ اور موسیٰؑ کے صحیفوں میں"
            }
        ]
    }
]