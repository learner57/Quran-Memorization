        "verses": [
            {
                "id": 1,
                "text": "قُلۡ يَـٰٓأَيُّهَا ٱلۡكَٰفِرُونَ",
                "translation": "کہہ دو کہ اے کافرو"
            },
            {
                "id": 2,
                "text": "لَآ أَعۡبُدُ مَا تَعۡبُدُونَ",
                "translation": "میں اُن کی عبادت نہیں کرتا جن کی عبادت تم کرتے ہو"
            },
            {
                "id": 3,
                "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
                "translation": "اور نہ تم اُس کی عبادت کرنے والے ہو جس کی عبادت میں کرتا ہوں"
            },
            {
                "id": 4,
                "text": "وَلَآ أَنَا۠ عَابِدٞ مَّا عَبَدتُّمۡ",
                "translation": "اور نہ میں اُن کی عبادت کرنے والا ہوں جن کی عبادت تم نے کی ہے"
            },
            {
                "id": 5,
                "text": "وَلَآ أَنتُمۡ عَٰبِدُونَ مَآ أَعۡبُدُ",
                "translation": "اور نہ تم اُس کی عبادت کرنے والے ہو جس کی عبادت میں کرتا ہوں"
            },
            {
                "id": 6,
                "text": "لَكُمۡ دِينُكُمۡ وَلِيَ دِينِ",
                "translation": "تمہارے لیے تمہارا دین ہے اور میرے لیے میرا دین"
            }
        ]
