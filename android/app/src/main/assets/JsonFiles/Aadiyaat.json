[
        {
        "id": 100,
        "name": "العاديات",
        "transliteration": "Al-'Adiyat",
        "translation": "دوڑنے والے گھوڑے",
        "type": "meccan",
        "total_verses": 11,
        "verses": [
            {
                "id": 1,
                "text": "وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا",
                "translation": "قسم ہے اُن (گھوڑوں) کی جو پھنکارے مارتے ہوئے دوڑتے ہیں"
            },
            {
                "id": 2,
                "text": "فَٱلۡمُورِيَٰتِ قَدۡحٗا",
                "translation": "پھر (اپنی ٹاپوں سے) چنگاریاں جھاڑتے ہیں"
            },
            {
                "id": 3,
                "text": "فَٱلۡمُغِيرَٰتِ صُبۡحٗا",
                "translation": "پھر صبح سویرے چھاپہ مارتے ہیں"
            },
            {
                "id": 4,
                "text": "فَأَثَرۡنَ بِهِۦ نَقۡعٗا",
                "translation": "پھر اس موقع پر گرد و غبار اڑاتے ہیں"
            },
            {
                "id": 5,
                "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا",
                "translation": "پھر اِسی حالت میں کسی مجمع کے اندر جا گھستے ہیں"
            },
            {
                "id": 6,
                "text": "إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ",
                "translation": "حقیقت یہ ہے کہ انسان اپنے رب کا بڑا ناشکرا ہے"
            },
            {
                "id": 7,
                "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ",
                "translation": "اور وہ خود اِس پر گواہ ہے"
            },
            {
                "id": 8,
                "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ",
                "translation": "اور وہ مال و دولت کی محبت میں بری طرح مبتلا ہے"
            },
            {
                "id": 9,
                "text": "۞أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ",
                "translation": "تو کیا وہ اُس حقیقت کو نہیں جانتا جب قبروں میں جو کچھ (مدفون) ہے اُسے نکال لیا جائے گا"
            },
            {
                "id": 10,
                "text": "وَحُصِّلَ مَا فِي ٱلصُّدُورِ",
                "translation": "اور سینوں میں جو کچھ (مخفی) ہے اُسے برآمد کر کے اس کی جانچ پڑتال کی جائے گی؟"
            },
            {
                "id": 11,
                "text": "إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ",
                "translation": "یقیناً اُن کا رب اُس روز اُن سے خوب باخبر ہوگا"
            }
        ]
    }
]
