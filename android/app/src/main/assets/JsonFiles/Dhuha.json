[
    
    {
        "id": 93,
        "name": "الضحى",
        "transliteration": "Ad-Duhaa",
        "translation": "دن کا چڑھنا",
        "type": "meccan",
        "total_verses": 11,
        "verses": [
            {
                "id": 1,
                "text": "وَٱلضُّحَىٰ",
                "translation": "قسم ہے روز روشن کی"
            },
            {
                "id": 2,
                "text": "وَٱلَّيۡلِ إِذَا سَجَىٰ",
                "translation": "اور رات کی جبکہ وہ سکون کے ساتھ طاری ہو جائے"
            },
            {
                "id": 3,
                "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
                "translation": "(اے نبیؐ) تمہارے رب نے تم کو ہرگز نہیں چھوڑا اور نہ وہ ناراض ہوا"
            },
            {
                "id": 4,
                "text": "وَلَلۡأٓخِرَةُ خَيۡرٞ لَّكَ مِنَ ٱلۡأُولَىٰ",
                "translation": "اور یقیناً تمہارے لیے بعد کا دور پہلے دور سے بہتر ہے"
            },
            {
                "id": 5,
                "text": "وَلَسَوۡفَ يُعۡطِيكَ رَبُّكَ فَتَرۡضَىٰٓ",
                "translation": "اور عنقریب تمہارا رب تم کو اتنا دے گا کہ تم خوش ہو جاؤ گے"
            },
            {
                "id": 6,
                "text": "أَلَمۡ يَجِدۡكَ يَتِيمٗا فَـَٔاوَىٰ",
                "translation": "کیا اس نے تم کو یتیم نہیں پایا اور پھر ٹھکانا فراہم کیا؟"
            },
            {
                "id": 7,
                "text": "وَوَجَدَكَ ضَآلّٗا فَهَدَىٰ",
                "translation": "اور تمہیں ناواقف راہ پایا اور پھر ہدایت بخشی"
            },
            {
                "id": 8,
                "text": "وَوَجَدَكَ عَآئِلٗا فَأَغۡنَىٰ",
                "translation": "اور تمہیں نادار پایا اور پھر مالدار کر دیا"
            },
            {
                "id": 9,
                "text": "فَأَمَّا ٱلۡيَتِيمَ فَلَا تَقۡهَرۡ",
                "translation": "لہٰذا یتیم پر سختی نہ کرو"
            },
            {
                "id": 10,
                "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنۡهَرۡ",
                "translation": "اور سائل کو نہ جھڑکو"
            },
            {
                "id": 11,
                "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ",
                "translation": "اور اپنے رب کی نعمت کا اظہار کرو"
            }
        ]
    }
]